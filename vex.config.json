{
    "commitBeforeGenerate": false,
    "jsonSchemaDir": "./output/jsonSchema",
    "rootDir": "./output",
    "srcDir": "./output/src",
    "openapiDir": "./output/openapi",
    "app": {
        "roles": [
            "admin",
            "user"
        ],
        "enableSwagger": true,
        "useUserSchema": true,
        "useObjectID": true,
        "allowApiCreateUpdate_id": false
    },
    "useOauth": {
        "google": true,
        "microsoft": false,
        "apple": false,
        "github": false
    }
}