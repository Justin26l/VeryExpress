{
    "commitBeforeGenerate": false,
    "jsonSchemaDir": "./output/jsonSchema",
    "rootDir": "./output",
    "srcDir": "./output/src",
    "openapiDir": "./output/src/openapi",
    "app": {
        "enableSwagger": true,
        "useUserSchema": true,
        "useObjectID": true,
        "allowApiCreateUpdate_id": false
    },
    "useOauth": {
        "google": true,
        "microsoft": false,
        "apple": false,
        "github": false,
        "facebook": false
    },
    "useRBAC": {
        "roles": [
            "visitor",
            "member",
            "admin"
        ],
        "default": "visitor",
        "schemaIncluded": [
            "user"
        ]
    },
    "sysDir": "./output/src/system"
}