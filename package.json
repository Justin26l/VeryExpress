{
    "name": "very-express",
    "version": "0.3.6",
    "description": "based on json schema and openapi, generate Express RestApi server with mongoDb ",
    "author": "justin26l",
    "license": "MIT",
    "main": "index.js",
    "bin": {
        "vex": "./dist/cli.js"
    },
    "scripts": {
        "build": "npm run lint && npm run compile && npm run minify && node ./src/copyTsTemplates.js",
        "dev": "ts-node ./src/cli.ts",
        "test": "node ./dist/index.js",

        "lint": "eslint ./src --ext .ts --fix",
        "compile": "ncc build ./src/cli.ts -o dist",
        "minify": "uglifyjs ./dist/index.js -c -m -o ./dist/index.js"
    },
    "keywords": [
        "jsonSchema",
        "express",
        "RestApi",
        "mongoDb",
        "mongoose",
        "json2mongoose"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Justin26l/VeryExpress.git"
    },
    "dependencies": {
        "@vercel/ncc": "^0.38.1",
        "express-session": "^1.17.3",
        "js-yaml": "4.1.0",
        "json-schema-to-openapi-schema": "^0.4.0",
        "json2mongoose": "^0.3.0",
        "minimist": "^1.2.8"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.17.10",
        "@types/js-yaml": "^4.0.9",
        "@types/minimist": "^1.2.5",
        "@types/node": "^20.10.5",
        "@types/passport": "^1.0.16",
        "@types/passport-google-oauth20": "^2.0.14",
        "@types/swagger-ui-express": "^4.1.6",
        "@typescript-eslint/eslint-plugin": "^6.18.1",
        "@typescript-eslint/parser": "^6.18.1",
        "dotenv": "^16.3.1",
        "eslint": "^8.56.0",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "helmet": "^7.1.0",
        "js-yaml": "^4.1.0",
        "json-schema-to-openapi-schema": "^0.4.0",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "swagger-ui-express": "^5.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3",
        "uglifyjs": "^2.4.11"
    }
}
